{
    "name": "@probed/core",
    "description": "A single-pass JSX-friendly dynamic tree backend.",
    "version": "1.1.0",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "unpkg": "dist/index.umd.js",
    "scripts": {
        "test": "jest --verbose",
        "ci": "pnpm run lint",
        "lint": "eslint --ext .ts src/",
        "licenseCheck": "license-checker",
        "build:clean": "shx rm -rf dist",
        "build:types": "tsc --emitDeclarationOnly --project tsconfig.prod.json",
        "build:rollup": "rollup -c",
        "build": "pnpm run build:clean && pnpm run build:types && pnpm run build:rollup && probed-package ./dist"
    },
    "author": {
        "name": "Francois Chabot",
        "url": "https://https://github.com/FrancoisChabot"
    },
    "type": "module",
    "license": "Apache-2.0",
    "devDependencies": {
        "@probed/packager": "^1.1.0",
        "@rollup/plugin-typescript": "^8.2.1",
        "@types/jest": "^26.0.22",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^26.6.3",
        "license-checker": "^25.0.1",
        "prettier": "^2.2.1",
        "rollup": "^2.45.2",
        "rollup-plugin-terser": "^7.0.2",
        "shx": "^0.3.3",
        "terser": "^5.6.1",
        "ts-jest": "^26.5.5",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "mangle": {
        "regex": "^_"
    },
    "dependencies": {
        "@types/node": "^14.14.41"
    }
}